<h2>Services</h2>
<table class="ui celled table">
  <thead>
    <tr>
      <th>Service Name</th>
      <th>Value</th>
      <th>Num Sessions</th>
      <th>Total Sessions</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |service|}}
      <tr>
        <td>{{service.name}}</td>
        <td>{{service.router}}</td>
        <td>{{service.sessions}}</td>
        <td>{{service.totalsessions}}</td>
      </tr>
    {{/each}}

  </tbody>
  <tfoot>

  </tfoot>
</table>
