{{md-btn text="Graph" action="toggleGraph" class="indigo darken-2"}}
{{md-btn text="Table" action="toggleTable" class="indigo darken-2"}}

{{#if isShowingGraph}}
{{high-charts chartOptions=chartOptions content=chartData theme=theme}}
{{/if}}
{{#if isShowingTable}}
<table class="bordered">
  <thead>
    <tr>
      <th>Duration</th>
      <th>No. Events Queued</th>
      <th>No. Events Executed</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |event|}}
      <tr>
        <td>{{event.duration}}</td>
        <td>{{event.queued}}</td>
        <td>{{event.executed}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{/if}}
